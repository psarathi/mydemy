{
  "name": "mydemy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node fetchCoursesScript.js && cp courses.json public/courses.json && next build",
    "build:nocache": "SKIP_COURSE_FETCH=true node fetchCoursesScript.js && cp courses.json public/courses.json && next build",
    "build:courses": "node fetchCoursesScript.js && cp courses.json public/courses.json",
    "upload:courses": "node uploadCoursesToCDN.js",
    "update:courses": "npm run build:courses && npm run upload:courses",
    "start": "next start -p 4000",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "tauri:dev": "tauri dev",
    "tauri:build": "npm run build && tauri build",
    "tauri:build:nocache": "npm run build:nocache && tauri build",
    "tauri:build:mac": "npm run build && tauri build --target aarch64-apple-darwin",
    "tauri:build:mac:nocache": "npm run build:nocache && tauri build --target aarch64-apple-darwin"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "hls.js": "^1.6.15",
    "kafkajs": "^2.2.4",
    "next": "^15.0.0",
    "next-auth": "^4.24.11",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/cli": "^2.9.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "colors": "^1.4.0",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^2.3.2"
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "bracketSpacing": false,
    "tabWidth": 4
  }
}
